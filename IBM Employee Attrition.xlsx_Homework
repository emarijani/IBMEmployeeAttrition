    "## 0 - 1000 = Below Average\n",
    "## 1001 - 2000 = Low       \n",
    "## 2001 - 5000 = Middle      \n",
    "## 5001 - 10000 = High\n",
    "## 10001 - 20000 = Vey High\n",
    "\n",
